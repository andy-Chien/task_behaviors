<?xml version="1.0" encoding="UTF-8"?>

<behavior name="Change Tool Model">

	<executable package_path="task_flexbe_behaviors.change_tool_model_sm" class="ChangeToolModelSM" />
	<tagstring>v0</tagstring>
	<author>Andy Chien</author>
	<date>Mon Jul 03 2023</date>
	<description>
		Change tool model for moveit, it will attached on tool base frame
	</description>


	<!-- Contained Behaviors -->

	<!-- Available Parameters -->
	<params>

		<param type="yaml" name="gripper_mesh_file" default="task_flexbe_behaviors/config/pick_and_place.yaml" label="gripper_mesh_file" hint="Sets the gripper_mesh_file">
			<key name="gripper_mesh_file" />
		</param>

		<param type="yaml" name="suction_mesh_file" default="task_flexbe_behaviors/config/pick_and_place.yaml" label="suction_mesh_file" hint="Sets the suction_mesh_file">
			<key name="suction_mesh_file" />
		</param>

		<param type="yaml" name="tool_touch_links" default="task_flexbe_behaviors/config/pick_and_place.yaml" label="tool_touch_links" hint="Sets the tool_touch_links">
			<key name="tool_touch_links" />
		</param>

		<param type="text" name="namespace" default="" label="namespace" hint="Sets the namespace" />

	</params>


</behavior>