<?xml version="1.0" encoding="UTF-8"?>

<behavior name="Move To Pick">

	<executable package_path="task_flexbe_behaviors.move_to_pick_sm" class="MoveToPickSM" />
	<tagstring>v0</tagstring>
	<author>Andy Chien</author>
	<date>Fri Jun 30 2023</date>
	<description>
		Move robot to Pick object
	</description>


	<!-- Contained Behaviors -->
	<contains name="Move Arm To Pose Async" />

	<!-- Available Parameters -->
	<params>

		<param type="text" name="io_service" default="/ur_hardware_interface/set_io" label="io_service" hint="Sets the io_service" />

		<param type="boolean" name="sim" default="True" label="sim" hint="Sets the sim" />

		<param type="yaml" name="joint_names" default="task_flexbe_behaviors/config/joint_names.yaml" label="joint_names" hint="Sets the joint_names">
			<key name="joint_names" />
		</param>

		<param type="boolean" name="select_tool_by_input" default="False" label="select_tool_by_input" hint="Sets the select_tool_by_input" />

		<param type="text" name="namespace" default="" label="namespace" hint="Sets the namespace" />

		<param type="text" name="group_name" default="ur_manipulator" label="group_name" hint="Sets the group_name" />

	</params>


</behavior>